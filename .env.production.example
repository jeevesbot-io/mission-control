# Mission Control — Production Environment
# Copy to .env.production and fill in real values.
# Generate a session secret with:
#   python3 -c "import secrets; print(secrets.token_urlsafe(32))"

DATABASE_URL=postgresql+asyncpg://jeeves@host.docker.internal:5432/jeeves
PORT=5050
DEBUG=false
SESSION_SECRET=REPLACE_ME_WITH_REAL_SECRET
CORS_ORIGINS=http://localhost:5050
MEMORY_PATH=/memory
OPENCLAW_URL=http://host.docker.internal:18789

# Calendar — empty disables gog CLI (graceful degradation in Docker)
GOG_PATH=

# Cloudflare Access — leave empty to disable (local/Tailscale access only)
CF_ACCESS_TEAM=
CF_ACCESS_AUDIENCE=
